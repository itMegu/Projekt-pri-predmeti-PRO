{
    "name" : "Google Addon project",
    "description" : "Å olski projekt za prograrmiranje",
    "version" : "1.0",
    "manifest_version" : 3,
    "background":{
        "scripts_background" :["js/background.js"]
    },
    "permissions" :[
        "storage",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "action":{
        "default_popup" : "popup.html",
        "default_icon": {
            "16" : "/img/get_started16.png",
            "32" : "/img/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    }
}